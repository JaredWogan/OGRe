  RowBox[{
   RowBox[{
    SubscriptBox["\[Del]", "\[Mu]"], 
    SuperscriptBox["G", "\[Mu]\[Nu]"]}], "=", "0"}]], "InlineFormula",
  CellChangeTimes->{{3.8207050870978355`*^9, 3.820705167702216*^9}, {
