Cell[BoxData[
 RowBox[{"TNewMetric", "[", 
  RowBox[{"\"\<Minkowski\>\"", ",", "\"\<Cartesian\>\"", ",", 
   RowBox[{"DiagonalMatrix", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", "1"}], ",", "1", ",", "1", ",", "1"}], "}"}], "]"}], ",", 
   "\"\<\[Eta]\>\""}], "]"}]], "Input",
