 RowBox[{
  SubscriptBox["\[DottedSquare]", "\[Mu]\[Nu]"], "=", 
  RowBox[{
   SubscriptBox["\[Eta]", "\[Mu]\[Nu]"], "+", 
   SubscriptBox["T", "\[Mu]\[Nu]"], "+", 
   SubscriptBox["N", "\[Mu]\[Nu]"], "+", 
   SubscriptBox["N", 
    RowBox[{
    "\[Nu]\[Mu]", 
     ","}]]}]}]], \
"DisplayFormula",ExpressionUUID->"f67086f6-dd53-4563-ac76-6d58a66dd6b9"],

