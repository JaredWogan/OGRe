Cell[BoxData[
 RowBox[{"TShow", "@", 
  RowBox[{"TCalc", "[", 
   RowBox[{"\"\<Kretschmann\>\"", ",", 
    RowBox[{
     RowBox[{
     "\"\<SchwarzschildRiemann\>\"", "[", "\"\<\[Rho]\[Sigma]\[Mu]\[Nu]\>\"", 
      "]"}], ".", 
     RowBox[{
     "\"\<SchwarzschildRiemann\>\"", "[", "\"\<\[Rho]\[Sigma]\[Mu]\[Nu]\>\"", 
      "]"}]}], ",", "\"\<K\>\""}], "]"}]}]], "Input",
 CellLabel->
