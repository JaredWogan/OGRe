     RowBox[{"\"\<Minkowski\>\"", "[", "\"\<\[Mu]\[Nu]\>\"", "]"}], "+", 
     RowBox[{"\"\<PerfectFluid\>\"", "[", "\"\<\[Mu]\[Nu]\>\"", "]"}], "+", 
     RowBox[{"\"\<NonSymmetric\>\"", "[", "\"\<\[Mu]\[Nu]\>\"", "]"}], "+", 
     RowBox[{"\"\<NonSymmetric\>\"", "[", "\"\<\[Nu]\[Mu]\>\"", "]"}]}], 
