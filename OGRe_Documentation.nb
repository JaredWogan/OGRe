                "\"t\"", ShowContents -> False, StripOnInput -> False]}, 
               "RowDefault"]}, "Subsuperscript", SyntaxForm -> 
             SubsuperscriptBox]},
           "RowWithSeparators"], "\<\"=\"\>", 
          RowBox[{"-", "\[Rho]"}]},
         {
          TemplateBox[{"=", "\"=\"", 
            TemplateBox[{"\"T\"", 
              TemplateBox[{
                StyleBox[
                "\"x\"", ShowContents -> False, StripOnInput -> False], 
                "\"x\""}, "RowDefault"], 
              TemplateBox[{"\"x\"", 
                StyleBox[
                "\"x\"", ShowContents -> False, StripOnInput -> False]}, 
               "RowDefault"]}, "Subsuperscript", SyntaxForm -> 
             SubsuperscriptBox], 
            TemplateBox[{"\"T\"", 
              TemplateBox[{
