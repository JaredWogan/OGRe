         {
          TemplateBox[{"=", "\"=\"", 
            TemplateBox[{"\"g\"", 
              TemplateBox[{
                StyleBox[
                "\"t\"", ShowContents -> False, StripOnInput -> False], 
                "\"t\""}, "RowDefault"], 
              TemplateBox[{"\"t\"", 
                StyleBox[
                "\"t\"", ShowContents -> False, StripOnInput -> False]}, 
